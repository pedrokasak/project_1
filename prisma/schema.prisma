// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Item {
  id_item     BigInt @unique
  category    String
  description String
  price       Float
  width       Int
  heigth      Int
  length      Int
  weigth      Int
}
